{
  "request": {
    "method": "POST",
    "url": "/graphql-advanced",
    "customMatcher": {
      "name": "GraphqlOperationRequestMatcher",
      "parameters": {
        "query": "query Country($code: String!) { country(code: $code) { name code } }",
        "operationName": "Country",
        "variables": {
          "code": "AD"
        }
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "data": {
        "country": { "name": "Andorra", "code": "AD" }
      }
    }
  }
}
